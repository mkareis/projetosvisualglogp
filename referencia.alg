algoritmo "semnome"
var
   x, y: Inteiro
Procedimento soma(var a, b: inteiro)
inicio
   a<- a+1
   b<- b +2
   Escreval("Valor de A", a)
   Escreval("Valor de B", b)
   Escreval("A soma de A e B é", a+b)
FimProcedimento
inicio
   x<- 4
   y<- 8
   soma(x, y)
   Escreval("O valor de X", x)
   Escreval("O valor de Y", y)
   
fimalgoritmo
